<?xml version="1.0"?>
<!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
	<head>
		<style type="text/css">
			table {
			font-family:"Arial","sans-serif",stsong;
			font-size:8.5pt;
			table-layout: fixed;
			word-wrap:break-word;
			word-break:break-all;
			}
			.enaddress{
			font-family:"Arial","sans-serif";
			}
			.bold{
			font-weight: bold;
			}
			.mainrow{
			border-bottom: 1px solid #000;
			padding:0px;
			}
			.specialrow{
			border-bottom: 1px solid #000;
			border-top: 1px solid #000;
			padding:0px;
			}
			.specialrow>td{
			border-left: 1px solid #000;
			border-right: 1px solid #000;
			}
			.wholeborder{
			border-left:1px solid #000;
			border-top:1px solid #000;
			border-bottom:1px solid #000;
			padding:0px;
			}
			.subtablemainrow td{
			border-right: 1px solid #000;
			border-bottom: 1px solid #000;

			}
			.subtablemainrowtop{
			border-top: 1px solid #000;
			}
			.lastrow td{
			border-right: 1px solid #000;
			}
			.firsttd{
			border-left: 1px solid #000;
			padding-left: 10px;
			}
			.firsttdr{
			border-right: 1px solid #000;
			}
			.descriptionColumn{
			width:25%;
			}
			img{
			width:50;
			height:50;
			}
			.acbo td{
			border-bottom: 1px solid #000;
			}
			.topm td{
			border-top: 1px solid #000;
<!--			padding-top : 30px ;-->
			}
			.topp td{
			border-top: 1px solid rgb(252, 252, 252);
			padding-top: 10px;
			}
			.backg td{
				background-color: #1C6386;
<!--				color: rgb(253, 253, 250);-->
			}
			/* .ul td{
			border-bottom: 1px solid #000;
			} */
			/* .pdm td{
			padding-top: 30px;
			padding-bottom: 30px;
			} */
			.leftb{
			border-left:1 solid #000;
			}

		</style>
	</head>
	<body>
		<table width="100%">
			<tr>
				<td colspan="4" rowspan="{{companyTopRowspanNum}}" width="50%"><span><img height="100px" width="100px" src="{{companyLogo}}"></img></span></td>
				<td colspan="6" style="font-size:large" width="50%" align="right"><span>{{companyName}}</span></td>
			</tr>
			{{#companyInfoArr}}
			<tr>
				<td colspan="6" width="50%" align="right"><span>{{companyInfo}}</span></td>
			</tr>
			{{/companyInfoArr}}
		</table>
		<div>
			<span>__________________________________________________________________________________________</span>
		</div>
		<table width="100%">
			<tr width="100%">
				<td padding="0" width="100%">
					<table width="100%">
						<tr>
							<td align="center" colspan="10"><span class="bold" font-size="150%">Commercial Invoice</span></td>
						</tr>

						<tr>
							<td colspan="5" width="50%"><span class="bold">Invoice No. </span><span>{{piNo}}</span></td>
							<td align="right" colspan="5" width="50%"><span>Date: {{date}}</span></td>
						</tr>
						<tr class="backg">
							<td colspan="5" width="50%"><span class="bold" style="color:white">Customer Info: </span></td>
							<td colspan="5" width="50%"><span class="bold" style="color:white">Sales Contact:</span></td>
						</tr>

						<tr>
							<td colspan="5" width="50%">
								<span>
									{{#customerInfoArr}}
										{{customerInfo}}<br />
									{{/customerInfoArr}}
								</span>
							</td>
							<td colspan="5" width="50%">
								<span>
									{{#supplierInfoArr}}
									{{supplierInfo}}<br />
									{{/supplierInfoArr}}
								</span>
							</td>
						</tr>
						<tr>
							<td colspan="5" width="50%"><span></span></td>
							<td colspan="5" width="50%"><span></span></td>
						</tr>
						<tr>
							<td colspan="5" width="50%"><span></span></td>
							<td colspan="5" width="50%"><span></span></td>
						</tr>
						<tr>
							<td colspan="5" width="50%"><span></span></td>
							<td colspan="5" width="50%"><span></span></td>
						</tr>
						<tr>
							<td colspan="5" width="50%"><span></span></td>
							<td colspan="5" width="50%"><span></span></td>
						</tr>

						<!--产品明细信息-->
						<tr class="subtablemainrow topm">
							<td colspan="1" class="firsttd"><span class="bold">No.</span></td>
							<td colspan="3"><span class="bold">Item Description</span></td>
							<td colspan="1"><span class="bold">Qty</span></td>
							<td colspan="3"><span class="bold">Unit Price</span></td>
							<td colspan="2"><span class="bold">Ext. Price</span></td>
						</tr>
						{{#items}}
						<tr class="subtablemainrow">
							<td colspan="1" align="center" class="firsttd" vertical-align="center">{{no}}</td>
							<td colspan="3"><span>{{des}}</span></td>
							<td colspan="1"><span>{{qty}}</span></td>
							<td colspan="3"><span>{{currencyDis}}{{rate}}</span></td>
							<td colspan="2"><span>{{currencyDis}}{{ext}}</span></td>
						</tr>
						{{/items}}
						<!--采购明细行信息--><!--尾部-->
						<tr class="subtablemainrow">
							<td align="left" rowspan="7" colspan="5" width="100%" class="firsttd" style="white-space: pre-line;">
								<span>
									{{#markInfo}}
									{{remarkInfo}}<br />
									{{/markInfo}}
								</span>
							</td>
							<td colspan="3"><span>Sub Total </span></td>
							<td colspan="2"><span>{{currencyDis}}{{subSum}}</span></td>
						</tr>
						{{#emptyArr}}
						<tr class="subtablemainrow">
							<td colspan="3"><span>&ensp;</span></td>
							<td colspan="2"><span>&ensp;</span></td>
						</tr>
						{{/emptyArr}}
						{{#fyItems}}
						<tr class="subtablemainrow">
							<td colspan="3"><span>{{cpxh}}</span></td>
							<td colspan="2"><span>{{currencyDis}}{{ext}}</span></td>
						</tr>
						{{/fyItems}}
						<tr class="subtablemainrow">
							<td colspan="3"><span>Total</span></td>
							<td colspan="2"><span>{{currencyDis}}{{sum}}</span></td>
						</tr>
					</table>
				</td>
			</tr>
		</table>
		<table width="100%" style="margin-top: 30px;">
			<tr>
				<td colspan="10" align="center">
					<span>
						Company's Representative Officer Information and Signature on behalf Yejian Technologies Co.,Ltd
					</span>
				</td>
			</tr>
			<tr style="padding-top: 20px;padding-bottom: 20px;">
				<td colspan="10" align="center">
					<span>
						Signature: {{signature}}
					</span>
				</td>
			</tr>
			<tr class="topm">
				<td colspan="10" align="center">
					<span>
						Thanks for your business!
					</span>
				</td>
			</tr>
		</table>
	</body>
</pdf>
