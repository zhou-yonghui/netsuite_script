<?xml version="1.0"?>
<!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
	<head>
		<style type="text/css">
		table {
			font-family:"Arial","sans-serif",stsong;
			font-size:8.5pt;
			table-layout: fixed;
			word-wrap:break-word;
			word-break:break-all;
		}
		.enaddress{
			font-family:"Arial","sans-serif";
		}
		.bold{
			font-weight: bold;
		}
		.mainrow{
				border-bottom: 1px solid #000;
				padding:0px;
			  }
		.specialrow{
				border-bottom: 1px solid #000;
				border-top: 1px solid #000;
				padding:0px;
			  }
		.specialrow>td{
				border-left: 1px solid #000;
				border-right: 1px solid #000;
			  }
		.wholeborder{
				border-left:1px solid #000;
				border-top:1px solid #000;
				border-bottom:1px solid #000;
				padding:0px;
			  }
			  .subtablemainrow td{
				  border-right: 1px solid #000;
				  border-bottom: 1px solid #000;

			  }
			  .subtablemainrowtop{
			  border-top: 1px solid #000;
			  }
			  .lastrow td{
				  border-right: 1px solid #000;
			  }
			  .firsttd{
				  border-left: 1px solid #000;
				  padding-left: 10px;
			  }
			.firsttd2{
				border-left: 1px solid #000;
				padding-left: 10px;
				border-top: 1px solid #000;
			}
			  .firsttdr{
                  border-right: 1px solid #000;
			  }
			  .descriptionColumn{
				  width:25%;
			  }
			  img{
				width:50;
				height:50;
			  }
			  .acbo td{
				  border-bottom: 1px solid #000;
				  }
			  .topm td{
				  border-top: 1px solid #000;
				  padding-top : 10px ;
				  }
			  .topp td{
				border-top: 1px solid rgb(252, 252, 252);
				padding-top: 10px;
			  }
			  .backg td{
				  background-color: #1E6AA1;
				  color: rgb(253, 253, 250);
<!--			      color : #1E6AA1,-->
			  }
			  .mar td{
				  margin-bottom: 20px;
				  padding-top: 3px;
			  }
			  .mr td{
				  padding-top: 20px;
				  margin-bottom: 20px;
				  padding-left: 450px;
			  }
			.backgfir{
				border-top: 1px solid #000;
<!--				border-bottom: 1px solid #000;-->
<!--				border-right: 1px solid #000;-->
<!--				border-left: 1px solid #000;-->
			}
</style>
</head>
<body>
	<div>
		<span>
			<img src="https://7373203.app.netsuite.com/core/media/media.nl?id=3080&amp;c=7373203&amp;h=5hNwi5C8XZGafd6t7xWU29FHlLmCujKCxNLy5fLQbR9Y7pnA" width="90%" height="100px"></img>
		</span>
	</div>
    <table width="100%">
		<tr width="100%">
		<td padding="0" width="100%">
			<table width="100%">
				<tr class="topp">
					<td class="descriptionColumn" colspan="5" width="50%"><span class="bold">Consignee:</span></td>
					<td class="" colspan="2"><span class="bold" style="float:right">Invoice No.: </span></td>
					<td class="" colspan="3"><span style="float:left">{{piNo}}</span></td>
				</tr>
				<tr>
					<td colspan="5" width="50%">
						<span>
							{{#customerInfoArr}}
							{{customerInfo}}<br />
							{{/customerInfoArr}}
						</span>
					</td>
					<td colspan="2">
						<span class="bold" style="float:right">
							{{#piRight_arr}}
							{{rightTopInfo}}<br />
							{{/piRight_arr}}
						</span>
					</td>
					<td colspan="3">
						<span style="float:left">
							{{#piRight_arr}}
							{{rightInfo}}<br />
							{{/piRight_arr}}
						</span>
					</td>
				</tr>
				<tr>
					<td colspan="5" width="50%"><span></span></td>
					<td colspan="2"><span class="bold" style="float:right"></span></td>
					<td colspan="3"><span style="float:left"></span></td>
				</tr>
				<tr>
					<td colspan="5" width="50%"><span></span></td>
					<td colspan="2"><span class="bold" style="float:right"></span></td>
					<td colspan="3"><span style="float:left"></span></td>
				</tr>
				<tr>
					<td colspan="5" width="50%"><span></span></td>
					<td colspan="2"><span class="bold" style="float:right"></span></td>
					<td colspan="3"><span style="float:left"></span></td>
				</tr>
				<tr>
					<td colspan="5" width="50%"><span></span></td>
					<td colspan="2"><span class="bold" style="float:right"></span></td>
					<td colspan="3"><span style="float:left"></span></td>
				</tr>
				<!--产品明细信息-->
				<tr class="subtablemainrow backg">
					<td colspan="2" class="firsttd2" align="center"><span align="left" style="font-weight:bold;font-size:10px">PackageNo.(CTNS)</span></td>
					<td colspan="3" class="backgfir" align="center"><span style="font-weight:bold;font-size:10px">Description</span></td>
					<td colspan="2" class="backgfir" align="center"><span style="font-weight:bold;font-size:10px">Quantity (PCS/CTN)</span></td>
					<td colspan="1" class="backgfir" align="center"><span style="font-weight:bold;font-size:8px">Weight (KG)</span></td>
					<td colspan="2" class="backgfir" align="center"><span style="font-weight:bold;font-size:10px">Dimension (CM3)</span></td>
				</tr>
				{{#items}}
				<tr class="subtablemainrow">
					<td colspan="2" align="center" class="firsttd" vertical-align="center">{{no}}</td>
					<td colspan="3"><span>{{des}}</span></td>
					<td colspan="2"><span>{{qty}}</span></td>
					<td colspan="1"><span>{{weight}}</span></td>
					<td colspan="2"><span>{{dimension}}</span></td>
				
				</tr>
				{{/items}}
				<!--采购明细行信息--><!--尾部-->
				<tr class="subtablemainrow">
					<td align="left" colspan="5" width="100%" style="border-left: 1px solid #000;"><span font-size="90%" class="bold">TOTAL: {{packageSum}} PACKAGES </span></td>
					<td colspan="2">{{qtySum}}</td>
					<td colspan="1">{{weightSum}}</td>
					<td colspan="2">&#128507;</td>
				</tr>
				<tr class="mar">
					<td colspan="10"><div align="right"><span>
					On behalf of Hongkong Yejian Technologies Co.,Ltd
					</span></div></td>
				</tr>

				<tr rowspan="2">
					<td colspan="10" style="padding-bottom:0px">
						<div style="padding-left:530px;"><img width='' height='100px' src="{{dianziz}}"></img></div>
					</td>
				</tr>

				<tr>
					<td colspan="5">
						<span></span>
					</td>
					<td colspan="5" align='right' style="padding-top:0px">
						<div><span>Signature:  --------------------------------- </span></div>
					</td>
				</tr>

			</table>
		</td>
		</tr>
	</table>
	<div>
		<img src="https://7373203.app.netsuite.com/core/media/media.nl?id=39&amp;c=7373203&amp;h=uDoiUECgLaYJtAUrnQmkGOrN5bGc2w2nlSdIiiKFZU4q70-V&amp;fcts=20210922001746&amp;whence=" width="65%" height="100px"></img>
	</div>
</body>
</pdf>
